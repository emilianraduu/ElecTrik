  "scripts": {
    "start": "node server.js",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "test": "jest --verbose",
    "test:watch": "npm test -- --watch",
    "test:coverage": "jest --coverage"
  },
  "author": "Badita George & Radu Emilian",
  "license": "Facultatea de Informatica Iasi",
  "dependencies": {
    "leaflet": "^1.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "css-loader": "^0.26.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.10.0",
    "http-server": "^0.9.0",
    "jest": "^19.0.0",
    "node-sass": "^4.5.0",
